.wrapper
  .container
    %h1 Welcome
    = form_with  model: @user, url:new_user_registration_path, method: :get, class: '', local: true do |f|
      .form
        = link_to new_user_session_path, class: '' do
          Login
        = f.text_field :nickname, autofocus: true, autocomplete: "nickname",class:"",placeholder: "Nickname"
        = f.email_field :email, autofocus: true, autocomplete: "email", class: "", placeholder:"E-mail"
        = f.password_field :password, autofocus: true, autocomplete: "password", class: "", placeholder:"Password"
        = f.submit "Sign up", class: "", id: "login-button"
    %ul.bg-bubbles
      %li
      %li
